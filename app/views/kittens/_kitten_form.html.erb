<%= form_with(model: @kitten, local: true) do |f| %>
  <p><%= f.label :name %></p>
  <p><%= f.text_field :name %></p>

  <p><%= f.label :age %></p>
  <p><%= f.text_field :age  %></p>

  <p><%= f.label :cuteness %></p>
  <p><%= f.text_field :cuteness %></p>

  <p><%= f.label :softness %></p>
  <p><%= f.text_field :softness %></p>

  <p><%= f.submit "Create your kitten!" %>
<% end %>
